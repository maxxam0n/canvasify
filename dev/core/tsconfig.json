{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"resolveJsonModule": true
	},
	"include": ["**/*.ts"]
}
