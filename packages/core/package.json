{
	"name": "@maxxam0n/canvasify-core",
	"private": false,
	"version": "0.1.0",
	"description": "Core rendering engine for Canvasify",
	"keywords": [
		"canvas",
		"html5-canvas",
		"rendering",
		"shapes",
		"layers",
		"declarative",
		"graphics",
		"2d",
		"typescript",
		"framework-agnostic"
	],
	"license": "MIT",
	"type": "module",
	"main": "./dist/canvasify-core.umd.cjs",
	"module": "./dist/canvasify-core.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/canvasify-core.es.js",
			"require": "./dist/canvasify-core.umd.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"type-check": "tsc --noEmit",
		"lint": "eslint \"**/*.{ts,tsx,vue}\"",
		"prepublishOnly": "npm run build"
	}
}
